<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings>
    <!-- Rareburg API Settings -->
    <add key="Rareburg.ApiKey" value="API_KEY" />
    <add key="Rareburg.ApiEndPoint" value="API_ENDPOINT" />
    <add key="Rareburg.LocalFilePath" value="..\..\samples\all-articles.json" />
    
    <!-- Feed settings -->
    <add key="Feed.Format" value="rss" />

    <!-- Feed service settings -->
    <add key="FeedService.Title" value="Rareburg Article Feed" />
    <add key="FeedService.Description" value="Rareburg Knowledge Base" />
    <add key="FeedService.Category" value="Collectibles" />
    <add key="FeedService.BaseUri" value="https://www.rareburg.com/knowledge" />
    <add key="FeedService.ArticleUrl" value="https://www.rareburg.com/article/{0}" />
    <add key="FeedService.UserUrl" value="https://www.rareburg.com/user/{0}" />
    
    <!-- File Publisher settings -->
    <add key="FilePublisher.OutputFilePath" value="rareburg.articles.{0}.rss" />
    
    <!-- S3 Publisher settings -->
    <add key="S3Publisher.Region" value="eu-west-1" />
    <add key="S3Publisher.BucketName" value="BUCKET_NAME" />
    <add key="S3Publisher.FileName" value="rareburg.articles.rss" />
    <add key="S3Publisher.AccessKey" value="ACCESS_KEY" />
    <add key="S3Publisher.SecretKey" value="SECRET_KEY" />
  </appSettings>
</configuration>
